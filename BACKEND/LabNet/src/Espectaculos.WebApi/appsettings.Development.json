{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Seq", "Args": { "serverUrl": "http://localhost:5341", "apiKey": "REEMPLAZAR_CON_TU_API_KEY" } }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "ValidationTokens": {
    "Secret": "MiSuperSecreto123!",
    "Issuer": "MiApp",
    "Audience": "MiAppUsers"
  }
}
